{
  "manifest_version": 2,

  "name": "Cacher",
  "description": "this ext caches xmlhttprequests",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "css": ["mystyles.css"],
      "js": ["/bundle.js"],
      "run_at": "document_start"
    }
  ],
   "web_accessible_resources": ["/injected.js"]
}
